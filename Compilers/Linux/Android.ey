%e-yaml
---
Name: Android
Native: No
Maintainer: fundies
Target-platform: Android

Make-Vars:
  Path:
  make: make
  makeflags:
  cc: /opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/bin/clang
  cxx: /opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/bin/clang++
  cxxflags:
  links:

Parser-Vars:
  defines: /opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/bin/clang -dM -x c++ --std=c++03 -E $blank
  searchdirs: /opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/bin/clang -E -x c++ --std=c++03 -v $blank
  searchdirs-start: "#include <...> search starts here:"
  searchdirs-end: "End of search list."

EXE-Vars:
  resources: $exe
  Build-Extension:
  Run-output:
  Run-Program:
  Run-Params:
